<main class="user-index">
  <header>
    <h1>Members</h1>
  </header>

  <ul class="info-list">
    <% @users.each do |user| %>
      <li>
        <article class="user-box">
          <h2><%= link_to user.user_name, user_path(user) %></h2>
        </article>
      </li>
    <% end %>
  </ul>
</main>
